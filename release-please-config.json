{
  "packages": {
    ".": {
      "release-type": "node",
      "package-name": "ctrl-q-qvd-viewer",
      "changelog-path": "CHANGELOG.md",
      "bump-minor-pre-major": true,
      "bump-patch-for-minor-pre-major": true,
      "draft": false,
      "prerelease": false,
      "include-component-in-tag": false,
      "versioning": "default",
      "extra-files": [
        {
          "type": "json",
          "path": "package.json",
          "jsonpath": "$.version"
        }
      ]
    }
  },
  "release-search-depth": 100,
  "pull-request-title-pattern": "chore: release ${version}",
  "pull-request-header": "## Release Please",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "docs",
      "section": "Documentation",
      "hidden": false
    },
    {
      "type": "style",
      "section": "Styles",
      "hidden": true
    },
    {
      "type": "chore",
      "section": "Miscellaneous",
      "hidden": false
    },
    {
      "type": "refactor",
      "section": "Code Refactoring",
      "hidden": false
    },
    {
      "type": "test",
      "section": "Tests",
      "hidden": false
    },
    {
      "type": "build",
      "section": "Build System",
      "hidden": false
    }
  ]
}
